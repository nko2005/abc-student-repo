{
    "name": "Parasite Extension",
    "version": "1.0",
    "description": "An infectious browser experience",
    "manifest_version": 2,
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
      }
    ],
    "browser_action": {
      "default_icon": "MuleKick.png"


    },
    "background":{
      "scripts":["backgroundscript.js"]
    }
}
