{
    "name": "Counting extension",
    "version": "1.0",
    "description": "1,2,3 you know what I mean",
    "manifest_version": 2,
    "permissions": [

      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
      }
    ],
    "browser_action": {
      "default_icon": "tent.png",
      "default_popup": "popup/popup.html"
    },
    "background" :{

      "scripts": ["background.js"]
    }
}
