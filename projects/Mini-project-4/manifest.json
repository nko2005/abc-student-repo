{
    "name": "Study Mode",
    "version": "1.0",
    "description": "Enter the ultimate study environment",
    "manifest_version": 2,
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
      }
    ],
    "browser_action": {
      "default_icon": "tent.png",
      "default_popup": "popup/popup.html"
    }
}
